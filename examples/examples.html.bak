<head>
  <title>examples</title>
</head>

<body>

  {{#PaperDrawerPanel}}
    {{#PaperDrawerPanelMain}}
        {{#PaperHeaderPanel mode="standard" className="paper-header-panel-1 blue"}}
          {{#PaperToolbar className="paper-toolbar-0"}}
            {{#PaperToolbarTop}}
             {{>PaperIconButton icon="menu" className="paper-drawer-toggle"}}
              <span class="title">Toolbar Top</span>
            {{/PaperToolbarTop}}
          {{/PaperToolbar}}
          {{#PaperHeaderPanelMain}}
            <div class="content">

            <h2>Paper Elements</h2>

            <h3>Paper Tabs</h3>

            {{#PaperTabs selected=0 className="paper-tabs-0"}}
              {{#PaperTab className="paper-tab-0"}}
              one
              {{/PaperTab}}
              {{#PaperTab className="paper-tab-0"}}
              two
              {{/PaperTab}}
              {{#PaperTab className="paper-tab-0"}}
              three
              {{/PaperTab}}
            {{/PaperTabs}}

            <h3>Paper Checkbox</h3>
              {{>PaperCheckbox label="Oxygen" className="paper-checkbox-0"}}
              {{>PaperCheckbox label="Carbon" className="paper-checkbox-0"}}
              {{>PaperCheckbox label="Hydrogen" checked="checked" className="paper-checkbox-0"}}
              {{>PaperCheckbox label="Nitrogen" checked="checked" className="paper-checkbox-0"}}
              {{>PaperCheckbox label="Calcium" checked="checked" className="paper-checkbox-0"}}

            <h3>Paper Radio Button</h3>
              {{>PaperRadioButton label="Oxygen" className="paper-radio-button-0"}}
              {{>PaperRadioButton label="Carbon" className="paper-cradio-buttonheckbox-0"}}
              {{>PaperRadioButton label="Hydrogen" checked="checked" className="paper-radio-button-0"}}
              {{>PaperRadioButton label="Nitrogen" checked="checked" className="paper-radio-button-0"}}
              {{>PaperRadioButton label="Calcium" checked="checked" className="paper-radio-button-0"}}

            <h3>Paper Progress</h3>
            {{>PaperProgress className="paper-progress-0" value=30 min=0 max=100}}
            <h3>Paper Fab</h3>
            {{>PaperFab className="blue" icon="menu"}}
            {{>PaperFab className="yellow" icon="refresh"}}
            {{>PaperFab className="green" icon="more"}}
            {{>PaperFab icon="menu"}}
            <h3>Paper Button</h3>
            {{#PaperButton className="blue"}}Blue{{/PaperButton}}
            {{#PaperButton className="green"}}Green{{/PaperButton}}
            {{#PaperButton className="yellow"}}Yellow{{/PaperButton}}
            {{#PaperButton}}Default{{/PaperButton}}
            {{#PaperButton}}{{> IronIcon icon="refresh"}}Reload{{/PaperButton}}
            {{#PaperButton}}{{> IronIcon icon="chevronRight"}}Next{{/PaperButton}}
            <h3>Paper Spinner</h3>
            {{>PaperSpinner active='active' className="paper-spinner-0"}}
            {{>PaperSpinner active='active' className="paper-spinner-0"}}
            {{>PaperSpinner active='active' className="paper-spinner-0"}}
            {{>PaperSpinner active='active' className="paper-spinner-0"}}
            <h3>Paper Toolbar</h3>
            {{#PaperToolbar className="paper-toolbar-0"}}
              {{#PaperToolbarTop}}
                <span class="title">Toolbar Top</span>
              {{/PaperToolbarTop}}
            {{/PaperToolbar}}
            <br>
            {{#PaperToolbar className="paper-toolbar-1 medium-tall"}}
              {{#PaperToolbarTop}}
                {{>PaperIconButton icon="menu"}}
                <span class="title">Toolbar Top</span>
                {{>PaperIconButton icon="more"}}
              {{/PaperToolbarTop}}
              {{#PaperToolbarMiddle}}
                {{>PaperIconButton icon="refresh"}}
                <span class="title">Toolbar Middle</span>
                {{>PaperIconButton icon="more"}}
              {{/PaperToolbarMiddle}}
            {{/PaperToolbar}}


            <br>
            {{#PaperToolbar className="paper-toolbar-2 tall"}}
              {{#PaperToolbarTop}}
                {{>PaperIconButton icon="menu"}}
                <span class="title">Toolbar Top</span>
                {{>PaperIconButton icon="refresh"}}
                {{>PaperIconButton icon="more"}}
              {{/PaperToolbarTop}}
              {{#PaperToolbarMiddle}}
                {{>PaperIconButton icon="menu"}}
                <span class="title">Toolbar Middle</span>
                {{>PaperIconButton icon="refresh"}}
                {{>PaperIconButton icon="more"}}
              {{/PaperToolbarMiddle}}
              {{#PaperToolbarBottom}}
                {{>PaperIconButton icon="menu"}}
                <span class="title">Toolbar Bottom</span>
                {{>PaperIconButton icon="refresh"}}
              {{/PaperToolbarBottom}}
            {{/PaperToolbar}}
            <h3>Paper Icon Button</h3>
            {{>PaperIconButton icon="menu"}}
            {{>PaperIconButton icon="refresh"}}
            {{>PaperIconButton icon="more"}}
            <h3>Paper Material</h3>
            <div class="card">{{#PaperMaterial elevation=0}}elevation="0"{{/PaperMaterial}}</div>
            <div class="card">{{#PaperMaterial elevation=1}}elevation="1"{{/PaperMaterial}}</div>
            <div class="card">{{#PaperMaterial elevation=2}}elevation="2"{{/PaperMaterial}}</div>
            <div class="card">{{#PaperMaterial elevation=3}}elevation="3"{{/PaperMaterial}}</div>
            <div class="card">{{#PaperMaterial elevation=4}}elevation="4"{{/PaperMaterial}}</div>
            <div class="card">{{#PaperMaterial elevation=5}}elevation="5"{{/PaperMaterial}}</div>
            <h2>Iron Elements</h2>
            <h3>Iron Icon</h3>
            {{> IronIcon icon="menu"}}
            {{> IronIcon icon="refresh"}}
            {{> IronIcon icon="more"}}
            <h3>Iron Select</h3>
            {{#IronSelector selected=0 className="paper-selector-0"}}
              <div>Item  1</div>
              <div>Item  2</div>
              <div>Item  3</div>
              <div>Item  4</div>
            {{/IronSelector}}
            {{#IronSelector selected=2 className="paper-selector-0"}}
              <div>Item  1</div>
              <div>Item  2</div>
              <div>Item  3</div>
              <div>Item  4</div>
            {{/IronSelector}}
            {{#IronSelector selected=1 className="paper-selector-0"}}
              <div>Item  1</div>
              <div>Item  2</div>
              <div>Item  3</div>
              <div>Item  4</div>
            {{/IronSelector}}

            <div style="display:flex;flex-flow:row wrap">
            <div style="height:300px;width:300px;float:left;">
              {{#PaperHeaderPanel mode="standard" className="paper-header-panel-0 blue"}}
                <div class="paper-header">Standard</div>
                {{#PaperHeaderPanelMain}}
                <div class="content" style="height:2000px">content</div>
                {{/PaperHeaderPanelMain}}
              {{/PaperHeaderPanel}}
            </div>
            <div style="height:300px;width:300px;flex: 0 0 300px;">
              {{#PaperHeaderPanel mode="seamed" className="paper-header-panel-0 green"}}
                <div class="paper-header">Seamed</div>
                {{#PaperHeaderPanelMain}}
                <div class="content" style="height:2000px">content</div>
                {{/PaperHeaderPanelMain}}
              {{/PaperHeaderPanel}}
            </div>
            <div style="height:300px;width:300px;flex: 0 0 300px;">
              {{#PaperHeaderPanel mode="scroll" className="paper-header-panel-0 red"}}
                <div class="paper-header">Scroll</div>
                {{#PaperHeaderPanelMain}}
                <div class="content" style="height:2000px">content</div>
                {{/PaperHeaderPanelMain}}
              {{/PaperHeaderPanel}}
            </div>
            <div style="height:300px;width:300px;flex: 0 0 300px;">
              {{#PaperHeaderPanel mode="waterfall" className="paper-header-panel-0 yellow"}}
                <div class="paper-header">Waterfall</div>
                {{#PaperHeaderPanelMain}}
                <div class="content" style="height:2000px">content</div>
                {{/PaperHeaderPanelMain}}
              {{/PaperHeaderPanel}}
            </div>
            <div style="height:300px;width:300px;flex: 0 0 300px;">
              {{#PaperHeaderPanel mode="waterfall-tall" className="paper-header-panel-0 orange"}}
                <div class="paper-header">Waterfall-tall</div>
                {{#PaperHeaderPanelMain}}
                <div class="content" style="height:2000px">content</div>
                {{/PaperHeaderPanelMain}}
              {{/PaperHeaderPanel}}
            </div>
            <div style="height:300px;width:300px;flex: 0 0 300px;">
              {{#PaperHeaderPanel mode="cover" className="paper-header-panel-0 grey"}}
                <div class="paper-header">Cover</div>
                {{#PaperHeaderPanelMain}}
                <div class="content" style="height:2000px;background:#fff;margin:60px">content</div>
                {{/PaperHeaderPanelMain}}
              {{/PaperHeaderPanel}}
            </div>
          </div>

        </div>
      {{/PaperHeaderPanelMain}}
    {{/PaperHeaderPanel}}

    {{/PaperDrawerPanelMain}}
    {{#PaperDrawerPanelDrawer}}

      {{#PaperToolbar className="paper-toolbar-0"}}
        {{#PaperToolbarTop}}
          <span class="title">Drawer</span>
        {{/PaperToolbarTop}}

      {{/PaperToolbar}}
    {{/PaperDrawerPanelDrawer}}
  {{/PaperDrawerPanel}}
</body>

